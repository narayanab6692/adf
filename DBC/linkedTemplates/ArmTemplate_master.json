{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DBC"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"DBC_linked_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DBC_linked'"
		},
		"DBsizedData_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DBsizedData'"
		},
		"Lastmodified_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Lastmodified'"
		},
		"datamovesize_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'datamovesize'"
		},
		"greaterFileLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'greaterFileLS'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DBC_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"DBC_linked_connectionString": {
						"value": "[parameters('DBC_linked_connectionString')]"
					},
					"DBsizedData_connectionString": {
						"value": "[parameters('DBsizedData_connectionString')]"
					},
					"Lastmodified_connectionString": {
						"value": "[parameters('Lastmodified_connectionString')]"
					},
					"datamovesize_connectionString": {
						"value": "[parameters('datamovesize_connectionString')]"
					},
					"greaterFileLS_connectionString": {
						"value": "[parameters('greaterFileLS_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}